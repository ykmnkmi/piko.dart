{
    "name": "dartx",
    "displayName": "DartX",
    "description": "DartX language support for Visual Studio Code",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.54.0"
    },
    "bugs": {
        "url": "https://github.com/ykmnkmi/piko.dart/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ykmnkmi/piko.dart.git"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "dart",
        "html"
    ],
    "contributes": {
        "languages": [
            {
                "id": "dartx",
                "aliases": [
                    "DartX",
                    "dartx"
                ],
                "extensions": [
                    "dartx"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "dartx",
                "scopeName": "source.dartx",
                "path": "./syntaxes/dartx.json"
            }
        ]
    }
}